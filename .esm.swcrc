{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": ["types.ts"],
  "sourceMaps": true,
  "minify": true,
  "module": {
    "type": "es6"
  },
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "target": "es2019",
    "minify": {
      "compress": true,
      "mangle": true
    }
  }
}
