{
  "name": "@sardine/colour",
  "version": "1.0.3",
  "description": "It does things to colours",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/umd/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf ./dist && npm run build:spack && npm run build:esm && npm run build:umd",
    "build:esm": "npx swc ./src -d ./dist",
    "build:umd": "npx swc ./dist/umd/index.js --config-file .umd.swcrc -o ./dist/umd/index.umd.js",
    "build:spack": "npx spack",
    "test": "npm run build && c8 ava",
    "types:emit": "npx tsc --declaration --emitDeclarationOnly",
    "types:check": "npx tsc --noEmit",
    "publish:next": "npm run build && npm run types:emit && npm publish --tag next",
    "publish:latest": "npm run build && npm run types:emit && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardinedev/colour.git"
  },
  "files": [
    "dist",
    "!dist/tests"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "color",
    "colour"
  ],
  "author": "Hugo Nogueira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sardinedev/colour/issues"
  },
  "homepage": "https://github.com/sardinedev/colour#readme",
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.136",
    "ava": "^4.0.1",
    "c8": "^7.11.0",
    "typescript": "^4.5.5"
  }
}