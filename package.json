{
  "name": "@sardine/colour",
  "version": "1.1.1",
  "description": "It does things to colours",
  "module": "./dist/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs && npm run types:emit",
    "build:esm": "npx swc --config-file .esm.swcrc ./src -d ./dist",
    "build:cjs": "npx swc --config-file .cjs.swcrc ./src -d ./dist/cjs",
    "test": "rm -rf ./dist && npm run build:esm && c8 ava",
    "types:emit": "npx tsc --declaration --emitDeclarationOnly",
    "types:check": "npx tsc --noEmit",
    "publish:next": "npm run build && npm run types:emit && npm publish --tag next",
    "publish:latest": "npm run build && npm run types:emit && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardinedev/colour.git"
  },
  "files": [
    "dist",
    "!dist/tests"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "color",
    "colour"
  ],
  "author": "Hugo Nogueira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sardinedev/colour/issues"
  },
  "homepage": "https://github.com/sardinedev/colour#readme",
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.219",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "typescript": "^4.7.4"
  }
}